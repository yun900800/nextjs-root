{
  "name": "nextjs-root",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "fundamenta-dev": "yarn workspace nextjs-fundamental dev",
    "fundamenta-build": "yarn workspace nextjs-fundamental build",
    "prerendering-dev": "yarn workspace nextjs-prerendering dev",
    "prerendering-build": "yarn workspace nextjs-prerendering build",
    "prerendering-start": "yarn workspace nextjs-prerendering start",
    "serve-json": "json-server --watch db.json --port 4000",
    "api-dev": "yarn workspace nextjs-api dev",
    "api-build": "yarn workspace nextjs-api build",
    "api-start": "yarn workspace nextjs-api start",
    "style-dev": "yarn workspace nextjs-styling dev",
    "style-build": "yarn workspace nextjs-styling build",
    "style-start": "yarn workspace nextjs-styling start",
    "misc-dev": "yarn workspace next-misc dev",
    "misc-build": "yarn workspace next-misc build",
    "misc-start": "yarn workspace next-misc start",
    "misc-storybook": "yarn workspace next-misc storybook",
    "misc-build-storybook": "yarn workspace next-misc build-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "json-server": "^0.17.3",
    "reoverlay": "^1.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@storybook/addon-essentials": "7.4.2",
    "@storybook/addon-interactions": "7.4.2",
    "@storybook/addon-links": "7.4.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.4.2",
    "@storybook/nextjs": "^7.4.2",
    "@storybook/react": "7.4.2",
    "@storybook/react-webpack5": "7.4.2",
    "@storybook/testing-library": "0.2.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.4.2"
  }
}
